{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://adiwg.github.io/mdJson-schemas/schema/temporalExtent.json#",
  "type": "object",
  "title": "",
  "description": "A temporal boundary comprising all or a portion of the resource.",
  "example":"../examples/temporalExtent.json",
  "additionalProperties": true,
  "minProperties": 1,
  "oneOf": [{
    "title": "Type is instant.",
    "required": ["timeInstant"],
    "properties": {
      "timeInstant": {
        "$ref: "https://adiwg.github.io/mdJson-schemas/schema/./timeInstant.json#",
        "description": "Represents an identifiable position in time."
      }
    }
  }, {
    "title": "Type is period.",
    "required": ["timePeriod"],
    "properties": {
      "timePeriod": {
        "$ref: "https://adiwg.github.io/mdJson-schemas/schema/./timePeriod.json#"
      }
    }
  }]
}
